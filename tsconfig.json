{
  "compilerOptions": {
      "target": "ES6",
      "module": "CommonJS",
      "outDir": "dist",
      "sourceMap": true,
      "strict": true,
      "baseUrl": "./src",
      "esModuleInterop": true,  
      "forceConsistentCasingInFileNames": true,
      "skipLibCheck": true  
    },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"],
}